<% if not @ldap_sasl_authid_fix.nil? %>ldap_sasl_authid = <%= @ldap_sasl_authid_fix %><% end %>
<% if not @ldap_sasl_mech.nil? %>ldap_sasl_mech = <%= @ldap_sasl_mech %><% end %>
